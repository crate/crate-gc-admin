{
  "name": "@crate.io/crate-gc-admin",
  "version": "0.1.2",
  "author": "crate.io",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "export": "./dist/index.umd.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@crate.io/crate-ui-components": "^0.1.31",
    "ace-builds": "^1.32.2",
    "antd": "^4.24.14",
    "clsx": "^2.1.0",
    "cronstrue": "^2.47.0",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pretty-bytes": "^6.1.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.21.1",
    "sql-formatter": "^15.0.2",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.0",
    "web-vitals": "^2.1.0",
    "zustand": "^4.4.7"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build-lib": "tsc && vite build --config vite.config.lib.ts",
    "prepack": "yarn run build-lib",
    "test": "jest",
    "lint": "eslint --cache --ext=.ts --ext=.tsx src/",
    "link-local-lib": "yarn link @crate.io/crate-ui-components && yarn link react && yarn unlink react-dom && yarn add link:../crate-ui-components",
    "unlink-local-lib": "yarn remove @crate.io/crate-ui-components && yarn unlink @crate.io/crate-ui-components && yarn unlink react && yarn unlink react-dom && yarn add @crate.io/crate-ui-components",
    "link-local": "yarn link && cd node_modules/react && yarn link && cd ../react-dom && yarn link",
    "unlink-local": "yarn unlink && cd node_modules/react && yarn unlink && cd ../react-dom && yarn unlink"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "1.3.2",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.1",
    "vite-tsconfig-paths": "^4.2.3",
    "whatwg-fetch": "^3.6.20"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  }
}
