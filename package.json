{
  "name": "crate-gc-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@crate.io/crate-ui-components": "^0.1.23",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext=.ts --ext=.tsx src/",
    "link-local-lib": "yarn link @crate.io/crate-ui-components && yarn link react && yarn unlink react-dom && yarn add link:../crate-ui-components",
    "unlink-local-lib": "yarn remove @crate.io/crate-ui-components && yarn unlink @crate.io/crate-ui-components && yarn unlink react && yarn unlink react-dom && yarn add @crate.io/crate-ui-components"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3"
  }
}
